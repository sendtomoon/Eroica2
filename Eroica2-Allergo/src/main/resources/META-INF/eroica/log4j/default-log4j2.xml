<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info">
	<Appenders>
		<Console name="CONSOLE" target="SYSTEM_OUT">
			<PatternLayout pattern="[%d{HH:mm:ss}] [%-3p] %x  %c{1}:  %m%n" />
		</Console>
	</Appenders>

	<Loggers>
		<Root level="info" includeLocation="true">
			<AppenderRef ref="CONSOLE"/> 
		</Root>
	</Loggers>
</Configuration>



